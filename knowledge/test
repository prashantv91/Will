((alive ?x) (lisp (format t "alive ~A~%" ?x)) (not (kill ?y ?x)))
((kill me ?k) (lisp (format t "kill me ~A~%" ?k)) (action (hire me)))
((kill dragon sheep) (lisp (format t "kill dragon sheep~%")) (alive dragon))
((save ?x) (lisp (format t "save ~A~%" ?x)) (if (kill ?y ?x)) (kill ?z ?y))

((c ?x) (lisp (format t "c")) (action (c ?x)))

((b ?x) (lisp (format t "b")) (not (c ?x)) (action (b ?x)))

((d ?x) (lisp (format t "d")) (not (b ?x)))

